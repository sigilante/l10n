/+  l10n
=/  default-locale  `locale`[%en %$ %$]
;:  weld
:-  `locale`[%en %$ %$]
^-  (list [@tas tape])
:~  %autonym      "English"
    %welcome      "Hello"
    %goodbye      "Goodbye"
    %warning      "Warning"
    %error        "Error"
    %success      "Success"
    %man          "man"
    %woman        "woman"
    `@tas`'1'     "one"
    `@tas`'2'     "two"
    `@tas`'3'     "three"
    `@tas`'4'     "four"
    `@tas`'5'     "five"
    `@tas`'6'     "six"
    `@tas`'7'     "seven"
    `@tas`'8'     "eight"
    `@tas`'9'     "nine"
    `@tas`'10'    "ten"
